class ADT{
	private:
		int *p;
    	int size;
		int end; 
	public:
    	ADT(int);
		bool isFull();
		bool isEmpty();
		int GetFine();
		int GetInizio();
		int SommaElementi();
		int Trova(int);
		bool InserisciInPosizione(int, int);
		bool CancellaInPosizione(int);
};

class Coda:public ADT{
	private:
		int first;
	public:
		Coda(int);
		bool InserisciCoda(int);
		bool CancellaCoda(void);
};

class Pila:public ADT{
	private:
		int numIns;
		int numCanc;
	public:
		Pila(int, int, int);
		bool InserisciPila(int);
		bool CancellaPila(void);
};


#include<iostream>
#include "ADT1.h"
#include<stdlib.h>

using namespace std;

ADT::ADT(int dim){ 
	size=dim;     
	p=new int [size];
	end=-1;}

bool ADT::isFull(){
		return (end==size);}

bool ADT::isEmpty(){
		return (end==-1);}
	
int ADT::GetFine(){
	if(end!=-1){
		return p[end];}
		return end;}

int ADT::GetInizio(){
		if(isEmpty()){
		return -1;}
		return p[0];}


int ADT::Trova(int x){
	int cont=0;
	for(int i=0;i<end;i++)
		if(p[i]==x)
			cont++;
	return cont;	
}
int ADT::SommaElementi(){
	int somma=0, i;
	for(i=0;i<end;i++)
		somma+=p[i];
	return somma;
}

bool ADT::InserisciInPosizione(int elem, int indice){
	if(isFull())
		return false;
	if(isEmpty()){
		p[0]=elem;
		end=0;
		return true;
	}
	for(int i=end+1;i>indice;i--)
		p[i]=p[i-1];
	
	p[indice]=elem;
	return true;		
}
bool ADT::CancellaInPosizione(int indice){
	
	if(isEmpty()){
		return false;
	}
	for(int i=0;i<end;i++)
		p[i]=p[i+1];
	end--;
	return true;		
}
Coda(int x):ADT{
	first-1;
}
